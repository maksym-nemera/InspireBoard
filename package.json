{
  "name": "react-native-ts_template",
  "homepage": "https://maksym-nemera.github.io/react-native-ts_template",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "author": "Maksym Nemera",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "build-web": "expo export:web",
    "predeploy": "npm run build-web",
    "deploy": "gh-pages -d web-build",
    "deploy:force": "gh-pages -d web-build -f",
    "lint": "eslint . --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore --ignore-path .prettierignore",
    "fix-style": "npm run format && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,css,scss,html,tsx,jsx}": "npm run format",
    "*.{js,jsx,ts,tsx}": "npm run lint"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "color": "^4.2.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "expo": "~49.0.10",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-web": "^0.19.8",
    "react-redux": "^8.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/color": "^3.0.4",
    "@types/node": "^20.6.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "react-native-dotenv": "^3.4.9",
    "typescript": "^5.1.3"
  },
  "private": true
}
